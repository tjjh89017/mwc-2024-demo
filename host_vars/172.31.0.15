---
VPN:
  hostname: BRANCH
  interfaces:
    eth4:
      address: 10.1.3.2/24
      gateway: 10.1.3.1
    eth5:
      address: 10.2.3.2/24
      gateway: 10.2.3.1
      metric: 100
  bridge:
    address: 192.168.3.1/24
    member:
    - eth1
    - eth2
    - eth3
  dhcp:
    LAN1:
      subnet: 192.168.3.0/24
      gateway: 192.168.3.1
      range:
        start: 192.168.3.100
        stop: 192.168.3.200
  nat:
    wan:
    - eth4
    - eth5
